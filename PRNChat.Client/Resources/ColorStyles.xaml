<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!-- Pink Colors -->
    <SolidColorBrush x:Key="PrimaryPink" >#FF77B8</SolidColorBrush>
    <SolidColorBrush x:Key="DarkPink">#FE459F</SolidColorBrush>
    <SolidColorBrush x:Key="NeutralPink">#F1CDDE</SolidColorBrush>
    <SolidColorBrush x:Key="LightPink">#F9E6E9</SolidColorBrush>
    <SolidColorBrush x:Key="BackgroundPink">#FFF8F9</SolidColorBrush>

    <!-- Neutral Colors -->
    <SolidColorBrush x:Key="Black">#FF212121</SolidColorBrush>
    <SolidColorBrush x:Key="Gray">#FF757575</SolidColorBrush>
    <SolidColorBrush x:Key="LightGray">#FFBDBDBD</SolidColorBrush>
    <SolidColorBrush x:Key="White">#FFFFFF</SolidColorBrush>

</ResourceDictionary>